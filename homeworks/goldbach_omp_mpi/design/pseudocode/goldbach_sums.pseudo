shared struct goldbach_sums_t{
  capacity
  count
  numbers[]
  solution_count
}

shared struct number_t {
  id_responsible_process
  number
  capacity
  sum_count
  sums[]
}

function result(my_goldbach_sums) {
  for (i := 0 to my_goldbach_sums->count) {
    print my_goldbach_sums->numbers[i]->number, ": ", my_goldbach_sums->numbers[i]->sums
  }
}

function destroy(my_goldbach_sums) {
  delete everything stored in my_goldbach_sums
}